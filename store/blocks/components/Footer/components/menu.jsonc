{
    "flex-layout.row#footer-2-desktop": {
        "title": "Menu Rodapé Desktop",
        "children": [
            "flex-layout.col#atendimento",
            "vtex.menu@2.x:menu#institucional",
            "vtex.menu@2.x:menu#ajuda"
        ],
        "props": {
            "blockClass": "menu-footer",
            "paddingTop": 6,
            "paddingBottom": 3,
            "colSizing": "auto",
            "colGap": 9
        }
    },
    "vtex.menu@2.x:menu#institucional": {
        "title": "Menu Institucional",
        "props": {
            "orientation": "vertical",
            "blockClass": "menu-footer",
            "items": [
                {
                    "id": "menu-item-quem-somos",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/quem-somos",
                        "noFollow": false,
                        "tagTitle": "Quem Somos",
                        "text": "Quem Somos"
                    }
                },
                {
                    "id": "menu-item-cadastro",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/cadastro",
                        "noFollow": false,
                        "tagTitle": "Cadastro para Profissionais",
                        "text": "Cadastro para Profissionais"
                    }
                },
                {
                    "id": "menu-item-como-funciona",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/como-funciona",
                        "noFollow": false,
                        "tagTitle": "Como funciona",
                        "text": "Como funciona"
                    }
                },
                {
                    "id": "menu-item-contato",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/contato",
                        "noFollow": false,
                        "tagTitle": "Contato",
                        "text": "Contato"
                    }
                }
            ]
        },
        "children": [
            "rich-text#title-footer-menu-institucional"
        ]
    },
    "rich-text#title-footer-menu-institucional": {
        "title": "Titulo Menu Institucional",
        "props": {
            "text": "Institucional",
            "blockClass": "menu-footer"
        }
    },
    "vtex.menu@2.x:menu#ajuda": {
        "title": "Menu Ajuda Desktop",
        "props": {
            "orientation": "vertical",
            "blockClass": "menu-footer",
            "items": [
                {
                    "id": "menu-item-duvidas-frequentes",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/duvidas-frequentes",
                        "noFollow": false,
                        "tagTitle": "Dúvidas Frequentes",
                        "text": "Dúvidas Frequentes"
                    }
                },
                {
                    "id": "menu-item-politica-de-privacidade",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/politica-de-privacidade",
                        "noFollow": false,
                        "tagTitle": "Politica de Privacidade",
                        "text": "Politica de Privacidade"
                    }
                },
                {
                    "id": "menu-item-termos-e-condicoes",
                    "type": "custom",
                    "iconId": null,
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/termos-e-condicoes",
                        "noFollow": false,
                        "tagTitle": "Termos e Condições",
                        "text": "Termos e Condições"
                    }
                }
            ]
