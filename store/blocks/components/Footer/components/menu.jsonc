    "rich-text#title-footer-menu-ajuda": {
        "title": "Titulo Menu Ajuda",
        "props": {
            "text": "Ajuda",
            "blockClass": "menu-footer"
        }
    },
    "flex-layout.col#atendimento": {
        "children": [
            "rich-text#title-footer-menu-atendimento",
            "flex-layout.row#footer-container-link-whatsapp",
            "flex-layout.row#footer-container-link-email"
        ]
    },
    "rich-text#title-footer-menu-atendimento": {
        "title": "Titulo Menu atendimento",
        "props": {
            "text": "Atendimento",
            "blockClass": ["menu-footer", "atendimento"]
        }
    },
    "flex-layout.row#footer-container-link-whatsapp": {
        "children": [
            "link#footer-link-whatsapp",
            "image#footer-icon-whatsapp"
        ],
        "props": {
            "blockClass": "footer-container-link-cta",
            "colSizing": "auto",
            "preserveLayoutOnMobile": true,
            "colGap": 4
        }
    },
    "link#footer-link-whatsapp": {
        "title": "Link para whatsapp",
        "props": {
            "blockClass": "links-footer-cta",
            "href": "tel:1193214-8448",
            "label": "(11) 93214-8448"
        }
    },
    "image#footer-icon-whatsapp": {
        "props": {
            "blockClass": "footer-icon",
            "maxHeight": 24,
            "src": "assets/icons/whatsapp-light.svg"
        }
    },
    "flex-layout.row#footer-container-link-email": {
        "children": [
            "link#footer-link-email",
            "image#footer-icon-email"
        ],
        "props": {
            "blockClass": "footer-container-link-cta",
            "colSizing": "auto",
            "preserveLayoutOnMobile": true,
            "colGap": 4
        }
    },
    "link#footer-link-email": {
        "title": "Link para email",
        "props": {
            "blockClass": "links-footer-cta",
            "href": "mailto:contato@medbeauty.com.br",
            "label": "contato@medbeauty.com.br"
        }
    },
    "image#footer-icon-email": {
        "props": {
            "blockClass": "footer-icon",
            "maxHeight": 24,
            "src": "assets/icons/email-light.svg"
        }
    }
}